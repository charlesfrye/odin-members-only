{
  "name": "members-only",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build && npm run serve",
    "dev": "nodemon --ignore ./dist --exec 'heroku local' --signal SIGTERM",
    "test": "npx jest",
    "build": "babel src --out-dir dist && npm run build-css",
    "build-css": "postcss src/css/styles.css -o public/css/compiled.css",
    "watch-css": "nodemon --config nodemon-css.json",
    "serve": "node dist/index.js"
  },
  "dependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "autoprefixer": "^10.4.16",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "postcss": "^8.4.33",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": "18.x"
  }
}
